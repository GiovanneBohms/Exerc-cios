#include<stdio.h>
#include<stdlib.h>
#include<iostream>
using namespace std;

main(){
	float matriz[3][3];
	int contador = 0;
	
	for(int i = 0;i<3;i++ ){
		for (int j =0; j<3; j++){
			contador ++;
			matriz[i][j]=contador;
		
		}
	}
	
	for(int i = 0;i<3;i++ ){
		for (int j =0; j<3; j++){
			cout<<matriz[i][j];
		}
		cout<<endl;
	}
	// diagonal direita
	for(int i =0; i<3;i++){
		cout<<matriz[i][i];
	}
	
	//diagonal secundaria
	for(int i =0; i<3;i++){
		cout<<matriz[i][2-i];
		
	}
	

	
}
