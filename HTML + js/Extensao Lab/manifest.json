{
  "manifest_version": 3,
  "name": "Hello Extensions",
  "content_scripts": [
    {
      "matches": ["https://*.bbc.com/*"],
      "css": ["styles.css"],
      "js": ["content-script.js"]
    }
  ],
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "./popup/popup.html",
    "default_icon": "./images/icon48.png"
  }
}
